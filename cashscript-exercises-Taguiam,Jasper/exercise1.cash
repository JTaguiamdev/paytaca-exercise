pragma cashscript ^0.11.0;


contract Exercise1 (int min, int max) {

    /*
        make a contract where the user can only spend funds if:

        1. the function parameter number should be within range of the max and min
        2. max and min should be declared on the constructor
     */

    function spend(int number) {
        require(number >= min);
        require(number <= max);
    }
}