pragma cashscript ^0.11.0;


contract Exercise4 (pubkey ownerPubkey) {

    /*
        make a contract where the user can only spend funds if:

        1. hash of the owner's pubkey matches the public key hash spending parameter
     */

    function spend(bytes20 pubkeyHash) {
        require(hash160(ownerPubkey) == pubkeyHash);
    }
}